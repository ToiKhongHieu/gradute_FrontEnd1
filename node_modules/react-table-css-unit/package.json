{
  "name": "react-table-css-unit",
  "version": "0.1.1",
  "description": "column width support rest unit for react-table v7,such as %,vh,vw,etc.",
  "main": "build/index.js",
  "scripts": {
    "demo": "webpack serve --mode development --env development --config src/demo/webpack.config.js",
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf build",
    "test:eslint": "npx eslint --ext .tsx --ext .ts src/",
    "test:jest": "npx jest --env=jsdom --verbose",
    "test:jest:watch": "npx jest --watchAll --env=jsdom",
    "test": "run-s test:*",
    "release-patch": "npm version patch",
    "release-minor": "npm version minor",
    "release-major": "npm version major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsonz1993/react-table-css-unit.git"
  },
  "keywords": [
    "react-table",
    "css-unit"
  ],
  "author": "jsonz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsonz1993/react-table-css-unit/issues"
  },
  "homepage": "https://github.com/jsonz1993/react-table-css-unit#readme",
  "peerDependencies": {
    "react-table": "7.x"
  },
  "devDependencies": {
    "@testing-library/react": "^11.1.1",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-table": "^7.0.25",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "styled-components": "^5.2.1",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.5",
    "webpack": "^5.3.2",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-table": "7.x"
  }
}
